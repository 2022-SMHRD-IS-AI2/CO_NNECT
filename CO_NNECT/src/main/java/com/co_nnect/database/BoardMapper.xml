<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.co_nnect.model.BoardDAO">
   <insert id="upload" parameterType="com.co_nnect.model.BoardVO">
   insert into web_board values(
   num_board.nextval, #{title}, #{writer}, #{filename}, #{content}, sysdate)
   
   </insert>
   
   <select id="showBoard" resultType="com.co_nnect.model.BoardVO">
   	
   	select * from web_board
   	
   </select>
   
   <select id="detailBoard" resultType="com.co_nnect.model.BoardVO" parameterType="int">
   	
   	select * from web_board where num=#{num}
   	
   </select>

</mapper>